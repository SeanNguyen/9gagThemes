{
  "manifest_version": 2,

  "name": "9GAG Themes",
  "description": "This extension give you your favorite 9gag theme",
  "version": "0.1",

  "browser_action": {
    "default_icon": "app/assets/icon.png",
    "default_popup": "app/index.html",
    "default_title": "9GAG Theme"
  },

  "content_scripts": [
    {
      "matches": [
        "*://9gag.com/*"
      ],
      "css": ["app/themes/dark/theme.css"]
    }
  ],

  "permissions": [
  ]
}